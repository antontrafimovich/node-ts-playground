{
  "name": "payments-report",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "compile": "babel ./src --extensions .ts --out-dir dist",
    "build": "npm run compile",
    "watch-ts": "npm run compile -- -w",
    "watch-node": "nodemon ./dist/bin/www --inspect",
    "watch": "concurrently 'npm:watch-ts' 'npm:watch-node'",
    "create-map": "create-map"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.1.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "bin": {
    "create-map": "./dist/bin/create-map.js",
    "create-report": "./dist/bin/create-report.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  }
}
